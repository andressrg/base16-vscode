{
	"name": "Base16 Dark Ashes",
	"type": "dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#1C2023",
				"foreground": "#C7CCD1"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#C7CCD1"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#747C84"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.variable",
				"punctuation.definition.string",
				"punctuation.definition.parameters",
				"punctuation.definition.string",
				"punctuation.definition.array"
			],
			"settings": {
				"foreground": "#C7CCD1"
			}
		},
		{
			"name": "Delimiters",
			"scope": "none",
			"settings": {
				"foreground": "#C7CCD1"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#C7CCD1"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#C795AE"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "#C7AE95"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.require",
				"support.function.any-method"
			],
			"settings": {
				"foreground": "#AE95C7"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"support.class",
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#AEC795"
			}
		},
		{
			"name": "Classes",
			"scope": "meta.class",
			"settings": {
				"foreground": "#F3F4F5"
			}
		},
		{
			"name": "Methods",
			"scope": "keyword.other.special-method",
			"settings": {
				"foreground": "#AE95C7"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#C795AE"
			}
		},
		{
			"name": "Support",
			"scope": "support.function",
			"settings": {
				"foreground": "#95AEC7"
			}
		},
		{
			"name": "Strings, Inherited Class",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#95C7AE"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Floats",
			"scope": "none",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Boolean",
			"scope": "none",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#C7AE95"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Attribute IDs",
			"scope": [
				"entity.other.attribute-name.id",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AE95C7"
			}
		},
		{
			"name": "Selector",
			"scope": "meta.selector",
			"settings": {
				"foreground": "#C795AE"
			}
		},
		{
			"name": "Values",
			"scope": "none",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Headings",
			"scope": [
				"markup.heading punctuation.definition.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#AE95C7"
			}
		},
		{
			"name": "Units",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold",
				"punctuation.definition.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#AEC795"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic",
				"punctuation.definition.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C795AE"
			}
		},
		{
			"name": "Code",
			"scope": "markup.raw.inline",
			"settings": {
				"foreground": "#95C7AE"
			}
		},
		{
			"name": "Link Text",
			"scope": [
				"string.other.link",
				"punctuation.definition.string.end.markdown"
			],
			"settings": {
				"foreground": "#C7AE95"
			}
		},
		{
			"name": "Link Url",
			"scope": "meta.link",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#C7AE95"
			}
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#C7C795"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"background": "#565E65",
				"foreground": "#C7CCD1"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#95C7AE"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#C7AE95"
			}
		},
		{
			"name": "Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#C795AE"
			}
		},
		{
			"name": "Colors",
			"scope": "constant.other.color",
			"settings": {
				"foreground": "#95AEC7"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#95AEC7"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#95AEC7"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#C79595"
			}
		},
		{
			"name": "Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#C7AE95",
				"foreground": "#F3F4F5"
			}
		},
		{
			"name": "Broken",
			"scope": "invalid.broken",
			"settings": {
				"background": "#C7C795",
				"foreground": "#1C2023"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#C79595",
				"foreground": "#F3F4F5"
			}
		},
		{
			"name": "Unimplemented",
			"scope": "invalid.unimplemented",
			"settings": {
				"background": "#747C84",
				"foreground": "#F3F4F5"
			}
		}
	],
	"colors": {
		// Editor Colors
		"editor.background": "#1C2023", //primary
		"editorCursor.foreground": "#C7CCD1",
		"editor.foreground": "#C7CCD1",
		"editorWhitespace.foreground": "#747C84",
		"editor.lineHighlightBackground": "#747C8455",
		"editor.selectionBackground": "#747C84",
		// Workbench: Side Bar
		"sideBar.background": "#191c1f", // 2nd shade
		"sideBarTitle.foreground": "#C7CCD1",
		// Workbench: Activity Bar
		"activityBar.foreground": "#C7CCD1",
		"activityBar.background": "#323638", // 2nd tint
		"activityBarBadge.background": "#76797b", //5th tint
		// Workbench: Status Bar
		"statusBar.foreground": "#C7CCD1",
		"statusBar.background": "#323638",
		"statusBar.debuggingBackground": "#76797b",
		"statusBar.noFolderBackground": "#76797b",
		"statusBarItem.prominentBackground": "#a4a5a7", // 7th tint
		"statusBarItem.prominentHoverBackground": "#a4a5a799",
		// Workbench: Editor Groups
		"editorGroup.background": "#323638",
		"editorGroup.border": "#C7CCD1",
		"editorGroup.dropBackground": "#4a4543",
		"editorGroupHeader.tabsBackground": "#323638",
		"tab.activeBackground": "#1C2023",
		"tab.inactiveBackground": "#323638",
		// Workbench: Notifications
		"notification.background": "#6b6766",
		// Dropdown Control
		"dropdown.background": "#a5a2a2",
		// "dropdown.border": "",
		"dropdown.foreground": "#191c1f",
		// Button Control
		"button.background": "#4a4543",
		"button.foreground": "#C7CCD1",
		// Lists and Trees
		"list.activeSelectionBackground": "#323638",
		"list.dropBackground": "#323638",
		"list.focusBackground": "#323638",
		"list.hoverBackground": "#323638",
		"list.inactiveSelectionBackground": "#323638"
	}
}